profiles {

    cluster {
        process.executor = 'slurm'
        process.queue = 'urtgen_24hrs'
        process.memory = '20GB'
        executor.queueSize = 30
    }

    bigmem {
        process.executor = 'slurm'
        process.queue = 'urtgen_24hrs'
	process.memory = '60GB'
        executor.queueSize = 36
    }

    gpu {
        process.executor = 'slurm'
        process.queue = '$GPU_Q'
        process.memory = '40GB'
	process.clusterOptions = '--gres=gpu:1'
    }

}
